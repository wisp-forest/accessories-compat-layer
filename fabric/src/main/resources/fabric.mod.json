{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${list_of_authors}"
  ],
  "contributors": [
    "${list_of_contributors}"
  ],
  "contact": {
    "${contact_entry}": ""
  },
  "license": "${mod_license}",
  "icon": "assets/${mod_id}/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.example.example_mod.fabric.ExampleModFabricInit"
    ],
    "client": [
      "com.example.example_mod.fabric.client.ExampleModClientFabricInit"
    ]
  },
  "mixins": [
    "${mod_id}-fabric.mixins.json",
    "${mod_id}-common.mixins.json"
  ],
  "accessWidener": "${mod_id}-fabric.accesswidener",
  "depends": {
    "fabricloader": "${fabric_loader_version_range}",
    "fabric-api": "${fabric_api_version_range}",
    "minecraft": "${fabric_minecraft_version_range}",
    "owo": "${owo_fabric_version_range}"
  }
}